{
  "name": "css-to-shadow-style",
  "description": "CSS box-shadow to Layer Style Converter",
  "version": "1.3.1",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "css-to-shadow-style",
    "manifest": "src/manifest.json",
    "main": "css-to-shadow-style.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "Oliver Pitsch <oliver@pitsch.me>",
  "repository": "https://github.com/oliverpitsch/CSS-Shadow-to-Sketch-Style-Plugin"
}
